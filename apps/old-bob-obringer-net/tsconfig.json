{
  "extends": "@bob-obringer/tsconfig/tsconfig.nextjs.app.json",
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "${configDir}/next-env.d.ts",
    "${configDir}/next.config.js",
    "${configDir}/**/*.ts",
    "${configDir}/**/*.tsx",
    "${configDir}/.next/types/**/*.ts",
    ".next/types/**/*.ts"
  ]
}
