{
  "name": "@bob-obringer/monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean && rimraf node_modules",
    "deploy": "turbo run deploy --filter=docs^...",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "prepare": "husky",
    "release": "turbo run build --filter=docs^... && changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1"
  },
  "devDependencies": {
    "@bob-obringer/eslint-plugin": "workspace:*",
    "@bob-obringer/tsconfig": "workspace:*",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-turbo": "1.13.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.5.0",
    "prettier-plugin-tailwindcss": "0.5.14",
    "rimraf": "5.0.5",
    "tailwindcss": "workspace:*",
    "turbo": "1.13.3",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@8.15.7",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=8.15.7"
  },
  "pnpm": {
    "overrides": {
      "next": "14.2.3",
      "posthog-js": "1.129.0",
      "react": "18.3.0",
      "react-dom": "18.3.0",
      "tailwindcss": "3.4.3"
    }
  }
}
