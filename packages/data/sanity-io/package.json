{
  "name": "@bob-obringer/sanity-io",
  "version": "0.3.0",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "sanity build && pnpm build:extract && pnpm build:typegen && tsc -p tsconfig.package.json",
    "build:extract": "mkdir -p build && sanity schema extract --path ./build/sanity.schema.json --workspace=production",
    "build:typegen": "sanity typegen generate",
    "clean": "rimraf dist build node_modules .sanity",
    "deploy:development": "dotenv -e .env.development -- sanity deploy --workspace=development",
    "deploy:production": "dotenv -e .env.production -- sanity deploy --workspace=production",
    "dev": "sanity dev",
    "lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "migration": "sanity migration run migration",
    "migration:no-dry-run": "sanity migration run migration --no-dry-run",
    "start": "sanity start"
  },
  "dependencies": {
    "@sanity/cross-dataset-duplicator": "1.3.0",
    "@sanity/orderable-document-list": "1.2.1",
    "@sanity/vision": "3.42.1",
    "react": "workspace:*",
    "react-dom": "workspace:*",
    "sanity": "3.42.1",
    "styled-components": "6.1.9",
    "zod": "workspace:*"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "dotenv-cli": "7.4.2"
  }
}
