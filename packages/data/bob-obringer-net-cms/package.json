{
  "name": "@bob-obringer/bob-obringer-net-cms",
  "version": "0.0.1",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "types"
  ],
  "scripts": {
    "build": "pnpm sanity:build && pnpm sanity:extract && pnpm sanity:typegen && pnpm build:package",
    "build:package": "tsc -p tsconfig.package.json",
    "clean": "rimraf dist build node_modules .sanity types",
    "dev": "concurrently \"pnpm sanity:dev\" \"tsc -p tsconfig.package.json -w\"",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "sanity:deploy-graphql": "sanity graphql deploy",
    "sanity:dev": "sanity dev",
    "sanity:extract": "mkdir -p types && sanity schema extract --path ./types/sanity.schema.json",
    "sanity:start": "sanity start",
    "sanity:typegen": "sanity typegen generate"
  },
  "dependencies": {
    "@sanity/orderable-document-list": "1.2.1",
    "@sanity/vision": "3.40.0",
    "groq": "3.40.0",
    "react": "workspace:*",
    "react-dom": "workspace:*",
    "sanity": "3.40.0",
    "styled-components": "6.1.9"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "concurrently": "^8.2.2"
  }
}
