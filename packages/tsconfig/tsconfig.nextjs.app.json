{
  "extends": "./tsconfig.app.dom.json",
  "display": "bobs next.js tsconfig",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "jsx": "preserve",
    "noEmit": true,
    "incremental": true
  },
  "include": [
    "${configDir}/next-env.d.ts",
    "${configDir}/next.config.js",
    "${configDir}/**/*.ts",
    "${configDir}/**/*.tsx",
    "${configDir}/.next/types/**/*.ts"
  ],
  "exclude": ["${configDir}/node_modules"]
}
